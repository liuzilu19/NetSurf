# Debian/Ubuntu configuration for NetSurf Browser (LinuxFB)

override NETSURF_FB_FRONTEND := linux
override NETSURF_FB_FONTLIB := freetype

override NETSURF_USE_HARU_PDF := NO
override NETSURF_USE_MNG := NO
override NETSURF_USE_PNG := YES

override NETSURF_FB_RESPATH_linux := $(PREFIX)/share/netsurf-framebuffer/

